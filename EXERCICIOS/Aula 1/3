#include <stdio.h>
#include <stdlib.h>

int main(){

float nota1, nota2, nota3;
printf("Digite sua nota 1: ");
scanf("%f", &nota1);
printf("Digite sua nota 2: ");
scanf("%f", &nota2);
printf("Digite sua nota 3: ");
scanf("%f", &nota3);
printf("sua nota 3 sera: %f", (nota1+nota2+nota3)/3);
return 0;
}
