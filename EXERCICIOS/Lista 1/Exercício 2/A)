#include <stdio.h>
#include <stdlib.h>

int main () {
    float num1, num2, num3;
    printf("informe o primeiro numero: \n");
    scanf("%f", &num1);
    printf("informe o segundo numero: \n");
    scanf("%f", &num2);
    printf("informe o terceiro numero: \n");
    scanf("%f", &num3);
    printf("Os numeros invertidos: %f, %f, %f", num3, num2, num1);
    return 0;
}
