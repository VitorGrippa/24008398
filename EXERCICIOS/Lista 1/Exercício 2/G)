#include <stdio.h>

int main() {
    float P, V, T, M;
    printf("Digite a pressão do pneu (P): ");
    scanf("%f", &P);
    printf("Digite o volume do pneu (V): ");
    scanf("%f", &V);
    printf("Digite a temperatura do pneu (T em Fahrenheit): ");
    scanf("%f", &T);
    M = (P * V) / (0.37 * (T + 460));
    printf("A massa de ar no pneu é: %.2f\n", M);
    return 0;
}
