#include <stdio.h>

int main() {
    char nome[50];
    int idade, diasDeVida;
    printf("Digite o seu nome: ");
    scanf("%s", nome);
    printf("Digite a sua idade: ");
    scanf("%d", &idade);
    diasDeVida = idade * 365;
    printf("%s, vocÃª viveu aproximadamente %d dias. \n", nome, diasDeVida);
    return 0;
}
