#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main(){
   float nota_trab, aval_sem, exame, media;
   char conceito;
   printf("Informe a nota do Trabalho: \n");
   scanf("%f", &nota_trab);
   printf("Informe a nota da Avaliacao: \n");
   scanf("%f", &aval_sem);
   printf("Informe a nota do Exame: \n");
   scanf("%f", &exame);
   media=(nota_trab*2+aval_sem*3+exame*5)/10;

   if(media>=8){
    printf("A");
   }
   else if(media>=7){
    printf("B");
   }
   else if(media>=6){
    printf("C");
   }
   else if(media>=5){
    printf("D");
   }
   else if("media>=0"){
    printf("E");
   }
   else{
    printf("Essa nota nao existe");
   }
}
