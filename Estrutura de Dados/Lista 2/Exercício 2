#include <stdio.h>

int quad(int x) {
    return x * x;
}

int dobro(int x) {
    return x * 2;
}

int main() {
    int num;
    printf("Digite um numero inteiro: ");
    scanf("%d", &num);

    printf("Quadrado de %d = %d\n", num, quad(num));
    printf("Dobro de %d = %d\n", num, dobro(num));

    return 0;
}
