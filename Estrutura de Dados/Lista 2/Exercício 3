#include <stdio.h>

long long fatorial(int n) {
    if (n == 0 || n == 1) return 1;
    return n * fatorial(n - 1);
}

int main() {
    int n = 17; // 9 + 8 do RA 24008398
    printf("Calculo do fatorial de %d (soma dos 2 ultimos digitos do RA)\n", n);
    printf("%d! = %lld\n", n, fatorial(n));
    return 0;
}
