#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#define PI 3.14

int main()
{
    float a;
    int b;
    printf("Digite o valor de a: \n");
    scanf("%f", &a);
    printf("Digite o valor de b: \n");
    scanf("%d", &b);

    a -= b;
    printf("a = %.1f\n", a);

    a *= 4;
    printf("a = %.1f\n", a);

    a /= 3;
    printf("a = %.1f\n", a);

    a += 2;
    printf("a = %.1f\n", a);

    b %= 5;
    printf("b = %d\n", b);

    return 0;
}
